 163:lvgl/src/widgets/lv_roller.c ****     if(roller->mode == LV_ROLLER_MODE_INFINITE) {
 164:lvgl/src/widgets/lv_roller.c ****         int32_t sel_opt_signed = sel_opt;
 165:lvgl/src/widgets/lv_roller.c ****         uint16_t page = roller->sel_opt_id / LV_ROLLER_INF_PAGES;
